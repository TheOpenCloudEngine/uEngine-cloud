{
  "id": "/uengine-cloud-server",
  "cmd": null,
  "cpus": 0.5,
  "mem": 1024,
  "disk": 0,
  "instances": 1,
  "acceptedResourceRoles": [
    "*"
  ],
  "container": {
    "type": "DOCKER",
    "docker": {
      "forcePullImage": true,
      "image": "@host.registry.package@/uengine-cloud-server:v1",
      "parameters": [
        {
          "key": "label",
          "value": "APP_NAME=uengine-cloud-server"
        },
        {
          "key": "label",
          "value": "APP_TYPE=system"
        }
      ],
      "privileged": false
    },
    "volumes": [
      {
        "containerPath": "@pinpoint.agent-path@",
        "hostPath": "@pinpoint.agent-path@",
        "mode": "RW"
      }
    ],
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 0,
        "labels": {},
        "protocol": "tcp",
        "servicePort": 10005
      }
    ]
  },
  "env": {
    "DEPLOYMENT": "blue",
    "AGENT_USE": "@pinpoint.use@",
    "AGENT_PATH": "@pinpoint.agent-path@"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "ignoreHttp1xx": false,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "path": "/health",
      "portIndex": 0,
      "protocol": "HTTP",
      "timeoutSeconds": 20,
      "delaySeconds": 15
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external,internal",
    "HAPROXY_0_VHOST": "@host.cloud-server@"
  },
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "portDefinitions": []
}