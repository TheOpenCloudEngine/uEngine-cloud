{
  "id": "/uengine-redis",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "constraints": [
    [
      "hostname",
      "LIKE",
      "@redis.ip@"
    ]
  ],
  "container": {
    "portMappings": [
      {
        "containerPort": 6379,
        "hostPort": 0,
        "protocol": "tcp",
        "servicePort": 6379
      }
    ],
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/data",
        "hostPath": "/redis/datadir",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "redis",
      "forcePullImage": false,
      "privileged": false,
      "parameters": [
        {
          "key": "rm",
          "value": "true"
        },
        {
          "key": "label",
          "value": "APP_NAME=uengine-redis"
        },
        {
          "key": "label",
          "value": "APP_TYPE=system"
        }
      ]
    }
  },
  "cpus": 0.4,
  "disk": 0,
  "env": {

  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "timeoutSeconds": 20,
      "delaySeconds": 15,
      "protocol": "COMMAND",
      "command": {
        "value": "pwd"
      }
    }
  ],
  "instances": 1,
  "labels": {
    "HAPROXY_GROUP": "external,internal",
    "HAPROXY_0_MODE": "tcp"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 512,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "fetch": [],
  "args": [
    "redis-server",
    "--appendonly",
    "yes"
  ]
}